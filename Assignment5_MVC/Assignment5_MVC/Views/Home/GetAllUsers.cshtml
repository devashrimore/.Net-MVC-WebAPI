@model IEnumerable<Assignment5_MVC.Models.User>
@{
    ViewBag.Title = "Home Page";
}

<table width="50%">

    @foreach (var user in Model)

        {

            <tr>

                <td align="center">@user.UserName</td>

                <td align="center">@user.Password</td>

            </tr>

        }

</table>
